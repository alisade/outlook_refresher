{
	"name": "Outlook KeepAlive",
	"version": "0.2",
	"description": "Prevents the annoying logging out from Outlook Web Applications(OWA)",
	"browser_action": {
		"name": "Ooutlook KeepAlive",
		"default_icon":"images/38/refresh_gray.png"
	},
	"background":{
		"scripts":["js/background.js"]
	},
	"content_scripts":[{
		"matches":["<all_urls>"],
		"js":["js/content.js"]
	}],
	"permissions":["tabs","cookies","<all_urls>"],
	"manifest_version": 2
}